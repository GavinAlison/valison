<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://www.springframework.org/schema/beans" 
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
        http://www.springframework.osrg/schema/beans/spring-beans-4.2.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context-4.2.xsd">
	<context:component-scan base-package="com.alison.spring.ioc.t2" />
	<bean id="zoo" class="com.alison.spring.ioc.t2.Zoo" />
	<bean id="tiger" class="com.alison.spring.ioc.t2.Tiger" />
	<bean id="monkey" class="com.alison.spring.ioc.t2.Monkey" />
</beans>